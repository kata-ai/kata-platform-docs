    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "^6.1.0",
    "react-media-match": "^1.11.3",
    "remove-markdown": "^0.3.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "typeface-barlow": "^1.1.13",
    "typescript": "4.0.5",
    "utility-types": "^3.10.0"
  },
